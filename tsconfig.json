{
    "include": ["src/*", "test/*"],
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["ES2020", "DOM"],
      "types": ["jest"],
      "strictNullChecks": true,
      //Only for development
      "allowUnusedLabels": true,
      "allowJs": true,
      "esModuleInterop": true,
      "moduleResolution": "Node",
      "resolveJsonModule": true
    },
  }